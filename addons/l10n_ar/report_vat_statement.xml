    <record id="account_financial_report_l10n_ar_tva_line0" model="account.financial.report.line">
        <field name="name">Base Imponible</field>
        <field name="code">ARTAX00</field>
        <field name="formulas">balance = ARTAX01.balance + ARTAX05.balance</field>
        <field name="sequence" eval="1"/>
        <field name="financial_report_id" ref="account_financial_report_l10n_ar_tva0"/>
        <field name="level" eval="0" />
    </record>

