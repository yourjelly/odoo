<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="res_config_settings_view_form_inherit_l10n_in" model="ir.ui.view">
        <field name="name">res.config.settings.form.inherit.l10n_in</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="account.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div id="eu_service" position="after">
                <div class="col-xs-12 col-md-6 o_setting_box" title="Use Multi GSTIN without create multi companies.">
                    <div class="o_setting_left_pane">
                        <field name="group_l10n_in_multi_gstin"/>
                    </div>
                    <div class="o_setting_right_pane" name="l10n_eu_service_right_pane">
                        <label for="group_l10n_in_multi_gstin"/>
                        <div class="text-muted">
                            Use this if setup with more than one state and obtain multiple registrations.
                        </div>
                    </div>
                </div>
            </div>
        </field>
	</record>
</odoo>
