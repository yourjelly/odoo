<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="normal_zpl_label_hs_code" inherit_id="stock.normal_zpl_label">
        <xpath expr="//t[@t-if='price_included']" position="after">
            <t t-if="product.hs_code">
                ^FO300, 110
                ^A0N,15^FDHS:<t t-out="product.hs_code"/>^FS
            </t>
        </xpath>
    </template>
    <template id="alternative_zpl_label_hs_code" inherit_id="stock.alternative_zpl_label">
        <xpath expr="//t[@t-if='price_included']" position="after">
            <t t-if="product.hs_code">
                ^FO250, 90
                ^A0N,15^FDHS:<t t-out="product.hs_code"/>^FS
            </t>
        </xpath>
    </template>
</odoo>
