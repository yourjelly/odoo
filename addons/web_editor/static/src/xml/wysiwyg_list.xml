<?xml version="1.0" encoding="utf-8"?>
<templates>
    <we3-group t-name="wysiwyg.buttons.list">
        <we3-button name="list-ul" title="Unordered list"
            data-method="toggleList" data-value="ul">
            <i class="fa fa-list-ul"></i>
        </we3-button>
        <we3-button name="list-ol" title="Ordered list"
            data-method="toggleList" data-value="ol">
            <i class="fa fa-list-ol"></i>
        </we3-button>
        <we3-button name="list-checklist" title="Checklist"
            data-method="toggleList" data-value="checklist">
            <i class="fa fa-check-square"></i>
        </we3-button>
    </we3-group>
</templates>
